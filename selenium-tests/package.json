{
    "name": "aforo-selenium-tests",
    "version": "1.0.0",
    "description": "Selenium E2E tests for Aforo application",
    "scripts": {
        "test": "jest --config jest.config.js",
        "test:rateplan": "jest tests/rateplan*.test.js",
        "test:org": "jest tests/organization-complete-validation.test.js",
        "test:requirements": "jest tests/organization-requirements-validation.test.js",
        "test:headless": "HEADLESS=true jest --config jest.config.js",
        "test:watch": "jest --config jest.config.js --watch",
        "test:verbose": "jest --config jest.config.js --verbose"
    },
    "keywords": [
        "selenium",
        "e2e",
        "testing",
        "rateplan",
        "organization-form"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "selenium-webdriver": "^4.16.0"
    },
    "devDependencies": {
        "chromedriver": "^143.0.0",
        "jest": "^29.7.0"
    }
}
